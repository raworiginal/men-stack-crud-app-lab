<%- include('../partials/html-head', { title: 'All Songs' }) %>
<%- include('../partials/nav-bar') %>
<h1 class="title">All Songs</h1>
<ul>
  <% songs.forEach(song =>{ %>
  <li>
    <a href="/songs/<%=song._id %>"
      ><%= song.title %> by <%= song.artist %></a
    >
  </li>
  <% }); %>
</ul>
<a href="/songs/new">Add New Song</a>
</body>
</html>
